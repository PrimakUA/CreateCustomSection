<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Primak_CartData::css/cartData.css"/>
    </head>
    <body>
        <referenceContainer name="footer">
            <block class="Magento\Framework\View\Element\Template" name="primak.block" template="Primak_CartData::cartData.phtml" before="-">
                <arguments>
                    <argument name="viewModelCartData" xsi:type="object">Primak\CartData\ViewModel\CartData</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
